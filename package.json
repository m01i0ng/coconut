{
  "name": "coconut",
  "version": "1.0.0",
  "description": "Coconut is a static site generator.",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "serve": "serve",
    "prepare": "husky install",
    "test:unit": "vitest --ui",
    "test:e2e": "playwright test",
    "prepare:e2e": "tsx scripts/prepare-e2e.ts"
  },
  "keywords": [
    "coconut",
    "ssg",
    "vite",
    "react",
    "blog"
  ],
  "author": "m01i0ng <maliang1066@gmail.com>",
  "license": "MIT",
  "bin": {
    "coconut": "bin/coconut.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --ignore-unknown -w",
      "eslint --cache --fix"
    ],
    "*.{ts,tsx}": [
      "prettier --parser=typescript --ignore-unknown -w",
      "eslint --cache --fix"
    ]
  },
  "packageManager": "pnpm@8.10.5",
  "dependencies": {
    "@loadable/component": "^5.15.3",
    "@mdx-js/rollup": "^2.3.0",
    "@vitejs/plugin-react": "^4.0.0",
    "acorn": "^8.8.2",
    "cac": "^6.7.14",
    "chalk": "^5.2.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "github-slugger": "^2.0.0",
    "hast": "^1.0.0",
    "hast-util-from-html": "^2.0.0",
    "mdast-util-mdxjs-esm": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "rehype-autolink-headings": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^9.0.3",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-mdx": "^2.3.0",
    "remark-mdx-frontmatter": "^3.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^10.1.0",
    "remark-stringify": "^11.0.0",
    "shiki": "^0.14.1",
    "sirv": "^2.0.2",
    "unified": "^11.0.0",
