{
  "name": "coconut",
  "version": "1.0.0",
  "description": "Coconut is a static site generator.",
  "scripts": {
    "start": "tsup --watch",
    "build": "tsup",
    "serve": "serve",
    "prepare": "husky install",
    "test:init": "vitest --ui",
    "test:e2e": "playwright test",
    "prepare:e2e": "tsx scripts/prepare-e2e.ts"
  },
  "keywords": [
    "coconut",
    "ssg",
    "vite",
    "react",
    "blog"
  ],
  "author": "m01i0ng <maliang1066@gmail.com>",
  "license": "MIT",
  "bin": {
    "coconut": "bin/coconut.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier -w"
    ],
    "*.{ts,tsx}": [
      "prettier --parser=typescript -w"
    ]
  },
  "dependencies": {
    "@vitejs/plugin-react": "^3.1.0",
    "cac": "^6.7.14",
    "fs-extra": "^11.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^4.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@playwright/test": "^1.32.1",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.15.10",
    "@types/react": "^18.0.30",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitest/ui": "^0.29.7",
    "commitlint": "^17.5.0",
    "eslint": "^8.36.0",
    "eslint-config-alloy": "^4.9.0",
    "eslint-plugin-react": "^7.32.2",
    "execa": "^7.1.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "serve": "^14.2.0",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "typescript": "^4.9.5",
    "vitest": "^0.29.7"
  }
}
